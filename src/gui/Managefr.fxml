<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane styleClass="background-white" stylesheets="@../../../utils/MainStyle.css"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.ManageControllerfr">
    <StackPane layoutY="30.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <Text fx:id="topText" strokeType="OUTSIDE" strokeWidth="0.0" text="texte haut">
            <font>
                <Font name="Arial Bold" size="23.0"/>
            </font>
        </Text>
    </StackPane>
    <VBox layoutX="50.0" layoutY="80.0" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="50.0"
          AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="80.0">

        <HBox>

            <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Abonnement :">
                    <font>
                        <Font name="Arial" size="18.0"/>
                    </font>
                    <VBox.margin>
                        <Insets left="5.0"/>
                    </VBox.margin>
                </Text>
                <ComboBox fx:id="abonnementCB" promptText="Choisir abonnement" styleClass="form-control"/>

            </VBox>


            <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Joueur :">
                    <font>
                        <Font name="Arial" size="18.0"/>
                    </font>
                    <VBox.margin>
                        <Insets left="5.0"/>
                    </VBox.margin>
                </Text>
                <ComboBox fx:id="joueurCB" promptText="Choisir joueur" styleClass="form-control"/>

            </VBox>

        </HBox>

        <HBox>

            <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Entraineur :">
                    <font>
                        <Font name="Arial" size="18.0"/>
                    </font>
                    <VBox.margin>
                        <Insets left="5.0"/>
                    </VBox.margin>
                </Text>
                <ComboBox fx:id="entraineurCB" promptText="Choisir entraineur" styleClass="form-control"/>

            </VBox>


            <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Sujet :">
                    <font>
                        <Font name="Arial" size="18.0"/>
                    </font>
                    <VBox.margin>
                        <Insets left="5.0"/>
                    </VBox.margin>
                </Text>
                <TextField fx:id="sujetTF" alignment="TOP_LEFT" promptText="Saisir sujet" styleClass="form-control"/>
            </VBox>

        </HBox>

        <HBox>

            <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Date :">
                    <font>
                        <Font name="Arial" size="18.0"/>
                    </font>
                    <VBox.margin>
                        <Insets left="5.0"/>
                    </VBox.margin>
                </Text>
                <DatePicker fx:id="dateDP" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                            minHeight="-Infinity" minWidth="-Infinity" styleClass="form-control"/>

            </VBox>


            <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Heure :">
                    <font>
                        <Font name="Arial" size="18.0"/>
                    </font>
                    <VBox.margin>
                        <Insets left="5.0"/>
                    </VBox.margin>
                </Text>
                <TextField fx:id="heureTF" alignment="TOP_LEFT" promptText="Saisir heure" styleClass="form-control"/>
            </VBox>

        </HBox>

    </VBox>
    <Button fx:id="btnAjout" layoutX="265.0" layoutY="434.0" mnemonicParsing="false" onAction="#manage"
            prefHeight="51.0" prefWidth="150.0" styleClass="success-button" text="manipulation" textAlignment="CENTER"
            textFill="WHITE" wrapText="true" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="260.0"
            AnchorPane.rightAnchor="260.0"/>
</AnchorPane>